{
  "name": "skdesktop",
  "version": "1.0.12",
  "author": "Carlos S.",
  "description": "Skool Desktop",
  "private": true,
  "build": {
    "appId": "com.skdesktop.app",
    "productName": "Skool",
    "artifactName": "${productName}-Setup-${version}.${ext}",
    "publish": [
      {
        "provider": "github",
        "owner": "carlossanchezg",
        "repo": "skdesktop-update"
      }
    ],
    "icon": "build/sk512_O.png",
    "mac": {
      "category": "com.productivity.application",
      "target": [
        "zip",
        "dmg"
      ],
      "identity": "11D113134178E22FCD59595ABE6E58AD8AA42F7A",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "artifactName": "${productName}.${ext}"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false
    },
    "linux": {
      "target": [
        "zip",
        "AppImage"
      ]
    },
    "directories": {
      "buildResources": "release",
      "output": "release"
    }
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.1",
    "i18next": "^21.6.6",
    "lodash": "^4.17.21",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "realm": "^10.11.0",
    "sound-play": "^1.1.0",
    "update-electron-app": "^2.0.1",
    "uuid": "^8.3.2"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "build": "craco build",
    "start": "craco start",
    "prod": "craco build && electron .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder --win --mac -p never",
    "publish": "npm run build && electron-builder --win --mac --publish always"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@szhsin/react-menu": "^2.3.4",
    "axios": "^0.25.0",
    "concurrently": "^7.0.0",
    "electron": "^16.0.7",
    "electron-builder": "^22.14.5",
    "react": "^17.0.2",
    "react-bootstrap-dropdown-menu": "^1.1.23",
    "react-countdown-circle-timer": "^3.0.9",
    "react-datepicker": "^4.7.0",
    "react-dropdown": "^1.9.2",
    "react-i18next": "^11.16.1",
    "react-modal": "^3.14.4",
    "react-multi-select-component": "^4.2.3",
    "react-search-autocomplete": "^5.5.1",
    "react-switch": "^6.0.0",
    "spinners-react": "^1.0.6",
    "wait-on": "^6.0.0",
    "webpack-node-externals": "^3.0.0"
  }
}
